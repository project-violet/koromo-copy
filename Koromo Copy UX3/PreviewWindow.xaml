<Window x:Class="Koromo_Copy_UX3.PreviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Koromo_Copy_UX3"
           xmlns:domain="clr-namespace:Koromo_Copy_UX3.Domain"
        mc:Ignorable="d"
        Title="미리보기" Height="789" Width="805" WindowStyle="ToolWindow">
    <Grid>
        <local:WheelSpeedScrollViewer SpeedFactor="{Binding Source={x:Static Member=domain:SettingWrap.Instance}, Path=SearchSpaceWheelSpeed}">
            <StackPanel x:Name="ImageStack" HorizontalAlignment="Center"/>
        </local:WheelSpeedScrollViewer>
    </Grid>
</Window>
