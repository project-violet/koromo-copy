<Window x:Class="Koromo_Copy_UX3.ArtistViewerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Koromo_Copy_UX3"
        mc:Ignorable="d"
        Title="ArtistViewerWindow" Height="636" Width="1589.325">
    <Grid>
        <ScrollViewer>
            <WrapPanel x:Name="ArticlePanel">
                <WrapPanel.Resources>
                    <Style TargetType="{x:Type local:SearchSimpleElements}">
                        <Setter Property="Margin" Value="5"/>
                    </Style>
                </WrapPanel.Resources>
            </WrapPanel>
        </ScrollViewer>
    </Grid>
</Window>
